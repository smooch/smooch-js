{
  "env": {
    "host": {
      "presets": [
        "env",
        "stage-2",
        ["es2015", { "modules": false }]
      ],
      "plugins": [
        "transform-object-assign"
      ]
    },
    "frame": {
      "presets": [
        "env",
        "stage-2",
        "react",
        ["es2015", { "modules": false }]
      ],
      "plugins": [
        "transform-runtime",
        "transform-class-properties",
        "transform-es2015-classes"
      ]
    }
  }
}
